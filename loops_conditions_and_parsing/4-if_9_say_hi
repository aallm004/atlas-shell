#!/usr/bin/env bash
# write script that displays Best School 10 times w/ acception
counter=1
while [ $counter -lt 10 ]; do
        echo "Best School"
        counter=$(( Best School +1))
	if [ $counter -eq 8 ]
	then
		echo -e "Best School\nHi"
	else
		echo "Best School"
	fi
	let counter=counter+1

done
