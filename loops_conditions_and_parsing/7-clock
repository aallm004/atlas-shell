#!/usr/bin/env bash
# displaying the time for 12 hours and 59 min with bash script

hours=0

while [ $hours -lt 12 ]; do
    minutes=1
    while [ $minutes -le 59hour=0
minute=1

while [ $hour -le 12 ]
do
  while [ $minute -le 59 ]
  do
     echo "$hour:$minute"
     ((minute++))
  done
  ((hour++))
  minute=1
done | head -70 ]; do
        printf "%02d:%02d\n" $hours $minutes
        ((minutes++))
    done

    ((hours++))
done | head -n 70
